<?xml version="1.0" encoding="UTF-8"?>
<Cmds>
  <Cmd id="listento" type="internal">
    <CmdCallString>ListenTo</CmdCallString>
    <Description>Connect to provided tcp address. (only receives, does not transmit.) Syntax: "ListenTo 127.0.0.1:1234"</Description>
  </Cmd>
  <Cmd id="stoplisteningto" type="internal">
    <CmdCallString>StopListeningTo</CmdCallString>
    <Description>Stop listening to provided tcp address. `ListenTo` must have been called before to be able to use this command. "Syntax: StopListeningTo 127.0.0.1:1234"</Description>
  </Cmd>
  <Cmd id="help" type="internal">
    <CmdCallString>help</CmdCallString>
    <Description>Display the help menu. `help _cmd_` to display help to only _cmd_ or `help` for full list.</Description>
  </Cmd>
  <Cmd id="clearAuth" type="internal">
    <CmdCallString>clearAuth</CmdCallString>
    <Description>Server clears the clients authorization.</Description>
  </Cmd>
  <Cmd id="transmitimg" type="filetransfer">
    <CmdCallString>TransmitImg</CmdCallString>
    <Description>Send provided .elf image file to debug pc.</Description>
    <DestinationDir></DestinationDir>
  </Cmd>
  <Cmd id="sendconfig" type="filetransfer">
    <CmdCallString>SendConfig</CmdCallString>
    <Description>Sends provided ServerCmds.xml to server.</Description>
    <DestinationDir></DestinationDir>
  </Cmd>
  <Cmd id="sendfolder" type="filetransfer">
    <CmdCallString>SendFolder</CmdCallString>
    <Description>Sends provided folder contents to server.</Description>
    <DestinationDir></DestinationDir>
  </Cmd>
  <Cmd id="createDir" type="internal">
    <CmdCallString>createDir</CmdCallString>
    <Description>Creates provided folder dirs in the server's pre defined folder path.</Description>
  </Cmd>
</Cmds>